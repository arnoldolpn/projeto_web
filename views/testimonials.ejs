<!DOCTYPE html>
<html>

<head>
  <title><%= title %></title>

  <%- include('imports');-%>


</head>

<body>
  <%- include('Menu');-%>
  <div class="botao">
  <a href="/admin/testimonials/" class="btn btn-primary">Administração de Testimonials</a>
  </div>
  <div class="container">

    <% testimonials.forEach((item) => { %>

    <div class="card">
      <div class="card-header">
        <%= item.name %>
      </div>
      <div class="card-body">
        <blockquote class="blockquote mb-0">
          <p> <%= item.testimony %></p>
          <footer class="blockquote-footer"> <%= item.title %>. <br> <cite title="company"> <%= item.company %> </cite>
          </footer>
        </blockquote>
      </div>
    </div>

    <% }); %>


  </div>

</body>

</html>